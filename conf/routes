# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

GET     /                           controllers.Assets.at(file="doc/main.html")

GET    /test    controllers.ControllerTest.test

POST    /login      controllers.ControllerTest.login(request : Request)


GET    /me/pokomon controllers.PokemonController.allPokemon()

GET /map controllers.ControllerTest.map(request : Request)

# prefix must match `play.assets.urlPrefix`
GET     /assets/*file               controllers.Assets.at(file)
GET     /versionedAssets/*file      controllers.Assets.versioned(file)

->          /webjars                webjars.Routes
